<div class="row justify-content-center">
    <div class="col-sm-8 text-center border-top border-danger h5 py-3">Select options and press "Get Quote" 
    </div>  
</div>
<!-- row3 -->
<div class="row justify-content-center py-3">
    <div class="col-sm-4 bg-primary">
        right col for picture
    </div>
    <div class="col-sm-4">
        <span class="h3 border-bottom border-danger">Model Number</span>
        <p class="pt-3 h5">Description of Item goes here</p>
        <form action="/addquote" method="POST" class="py-3">
            <div>
                <select class="form-select form-select-lg mb-3" aria-label="Large select example" name="modelQuote">
                    <option selected name="modelNumber">model Number</option>
                    <!-- <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option> -->
                </select>
            </div>
            <div>
                <select class="form-select form-select-sm" aria-label="Small select example" name="service" id="option">
                    <option selected value="repair">Repair and Return</option>
                    <option value="reman"  name="reman">Remanufactured</option>
                    <option value="Exchange" name="exchange">Exchange</option>
                </select>
            </div>
        
              <div class="py-3">
                <button type="submit" class="btn btn-primary btn-danger">Get Quote</button>
            </div>
        </form>
    </div>
</div>
